<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<logfile name="cdn_log">
    <column index="1" cf="base" qualifier="host">host</column>
    <column index="2" cf="base" qualifier="begin_time">begin_time</column>
    <column index="3" cf="base" qualifier="end_time">end_time</column>
    <column index="4" cf="base" qualifier="url">cs-method cs-uri cs-version</column>
    <column index="5" cf="base" qualifier="sc_status">sc_status</column>
    <column index="6" cf="base" qualifier="bytes">bytes</column>
    <column index="7" cf="base" qualifier="cs_useragent">cs_useragent</column>
</logfile>
